        "## Manila bulletin\n",
        "# News link getter for National News\n",
        "# Simulate clicking the more button in MB site, and then extract all the news link\n",
        "\n",
        "site = \"manilaBulletin\"\n",
        "data=pd.DataFrame(columns=['Statement','Link','Date'])\n",
